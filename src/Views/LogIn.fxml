         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <Label layoutX="237.0" layoutY="148.0" text="Email" />
      <Label layoutX="237.0" layoutY="233.0" text="Mot de passe" />
      <TextField fx:id="emailTextInput" layoutX="237.0" layoutY="175.0" promptText="Entrer ton email" />
      <Button fx:id="btnConnect" layoutX="448.0" layoutY="337.0" mnemonicParsing="false" onAction="#seConnecter" text="Se Connecter" />
      <PasswordField fx:id="passwordInput" layoutX="237.0" layoutY="272.0" promptText="Entrer mot de passe" />
   </children>
