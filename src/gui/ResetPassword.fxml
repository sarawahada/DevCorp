   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label fx:id="rec" layoutX="87.0" layoutY="128.0" text="Your confirmation code was sent to you by email! Check your email and enter the code sent." />
            <Button fx:id="confirm" layoutX="255.0" layoutY="239.0" mnemonicParsing="false" onAction="#confirmCode" text="Confirm" />
            <TextField fx:id="cd" layoutX="212.0" layoutY="187.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
