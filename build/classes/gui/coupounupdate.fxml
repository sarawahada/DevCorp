<AnchorPane id="AnchorPane" prefHeight="521.0" prefWidth="414.0" stylesheets="@offre.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CoupounupdateController">
   <children>
      <ImageView fitHeight="521.0" fitWidth="414.0">
         <image>
            <Image url="@../pica/bk.png" />
         </image>
      </ImageView>
      <VBox layoutY="55.0" prefHeight="321.0" prefWidth="108.0" spacing="30.0">
