            <Label fx:id="name_label" text="Coupon Name :">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font></Label>
            <Label fx:id="date_label" text="Coupoun Code :">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font></Label>
            <Label fx:id="type_label" text="Start Date :">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font></Label>
            <Label fx:id="type_label1" text="Finish Date :">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font></Label>
            <Label fx:id="type_label11" text="Promotion ID :">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font></Label>
            <Label fx:id="type_label12" text="Duration :">
               <font>
                  <Font name="Century Gothic" size="12.0" />
               </font></Label>
         </children>
      </VBox>
      <VBox layoutX="114.0" layoutY="55.0" prefHeight="397.0" prefWidth="173.0" spacing="20.0">
         <children>
            <TextField fx:id="idc" />
            <TextField fx:id="namec" />
            <TextField fx:id="codec" />
            <TextField fx:id="date1_c" />
            <TextField fx:id="date2_c" />
            <TextField fx:id="id_promo" />
            <TextField fx:id="duree_c" />
         </children>
      </VBox>
      <Button layoutX="310.0" layoutY="55.0" mnemonicParsing="false" onAction="#getcoupoun" styleClass="add-btn" text="FIND" />
      <Button layoutX="115.0" layoutY="481.0" mnemonicParsing="false" onAction="#updatecoupoun" styleClass="add-btn" text="UPDATE" />
      <Button layoutX="207.0" layoutY="480.0" mnemonicParsing="false" onAction="#deletecoupoun" styleClass="add-btn" text="DELETE" />
   </children>
   
</AnchorPane>
